page {
  background-color: #1e1e1e;
}

.nav-bg {
  background: #3c3c3c;
}

.hover {
  background: #A2A2A2;
}

.fadeIn {
  animation: c 0.3s forwards;
  z-index: 9999;
}

@-webkit-keyframes c {
  0% {
      opacity: 0;
  }

  to {
      opacity: 1;
  }
}

@keyframes c {
  0% {
      opacity: 0;
  }

  to {
      opacity: 1;
  }
}

.divide {
  background: #A2A2A2;
  height: 18px;
  width: 0.5px;
}

.iconfont {
  font-size: 18px;
  text-align: center;
}

.icon-pause {
  color: var(--weui-YELLOW);
}

.icon-start {
  color: var(--weui-BRAND);
}