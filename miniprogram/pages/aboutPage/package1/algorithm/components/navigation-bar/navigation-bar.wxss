.navigation-bar {
    top: env(safe-area-inset-top);
    position: fixed;
    z-index: 9999;
    width: 100%;
}

.navigator {
    display: flex;
    flex-direction: row-reverse;
}

.navigator-hd {
    display: flex;
    border: 0.5px solid rgba(0,0,0,0.1);
    border-radius: 27px;
    background: rgba(255,255,255,0.6);
    align-items: center;
}

.navigator-hd.white {
    border-color: rgba(255,255,255,0.25);
    background: rgba(0,0,0,0.15);
}

.navigator-item:first-child {
    border-radius: 27px 0 0 27px;
}

.navigator-item {
    display: flex;
    flex: 1;
    align-items: center;
    justify-content: center;
    border-radius: 0 27px 27px 0;
}

.navigator-item.false {
    pointer-events: none;
}

.navigator-hd.white .iconfont {
    color: #ffffff;
}

.navigator-item.false .iconfont {
    color: rgba(255,255,255,0.18);
}

.capsule-sep {
    display: inline-block;
    min-width: 0.125px;
    height: 17.2px;
    background: rgba(255,255,255,0.25);
}

.navigator-bd {
    display: flex;
    align-items: center;
    justify-content: center;
}

.hover {
    background: #A2A2A2;
}

@font-face {
    font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 18px;
    line-height: 18px;
    font-style: bold;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-arrow-left:before {
    content: "\e75b";
}

.icon-home:before {
    content: "\e8f2";
}